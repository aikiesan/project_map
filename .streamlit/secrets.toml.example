# Streamlit Secrets Template for CP2B Maps V2
# Copy this file to 'secrets.toml' and fill in your actual values
# NEVER commit secrets.toml to version control!

# Example: Database credentials (if using external database)
# [database]
# host = "your-database-host"
# port = 5432
# username = "your-username"
# password = "your-password"
# database = "cp2b_maps"

# Example: API keys (if using external APIs)
# [api_keys]
# mapbox_token = "your-mapbox-token"
# google_maps_key = "your-google-maps-key"

# Example: Application settings
# [app]
# environment = "production"
# debug = false
# admin_email = "admin@example.com"

# Note: For this application, secrets are optional as we use local data files
# The application will work without any secrets configured